{
  "name": "belajar-nodejs-express",
  "version": "1.0.0",
  "description": "Belajar NodeJS expressJS",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "clearMocks": true,
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!vendor/**/*.{js,jsx}"
    ]
  },
  "author": "Thomas Alberto",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "babel-jest": "^29.6.1",
    "jest": "^29.6.1",
    "prettier": "3.0.1",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "express": "^4.18.2",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "cookie-parser": "^1.4.6",
    "mustache-express": "^1.3.2"
  }
}
